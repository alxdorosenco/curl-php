{
  "name": "alxdorosenco/curl-php",
  "description": "This is an easy builder of the cURL library in php",
  "keywords": [
    "php",
    "php-curl",
    "curl-php",
    "curl",
    "content",
    "open-source",
    "file",
    "post",
    "source"
  ],
  "homepage": "https://github.com/alxdorosenco/curl-php",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexei Dorosenco",
      "email": "alexforever1808@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "AlxDorosenco\\CurlPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlxDorosenco\\CurlPhp\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}